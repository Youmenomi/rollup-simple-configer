{
  "name": "rollup-simple-configer",
  "version": "0.1.2",
  "description": "Set up typescript rollup configuration quickly and easily.",
  "keywords": [
    "rollup",
    "simple",
    "configer",
    "build",
    "common"
  ],
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "build:clean": "del dist",
    "build": "tsc --noEmit && eslint --ext .ts,.js ./src && npm run build:clean && tsc -p tsconfig.build.json",
    "release": "np --no-yarn --no-cleanup",
    "version": "npm run build",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "eslint --fix",
      "jest --bail --findRelatedTests"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Youmenomi/rollup-simple-configer.git"
  },
  "author": "Dean Yao <youmenomi@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Youmenomi/rollup-simple-configer/issues"
  },
  "homepage": "https://github.com/Youmenomi/rollup-simple-configer#readme",
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@types/babel__core": "^7.1.14",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "del-cli": "^3.0.1",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "np": "^7.4.0",
    "pinst": "^2.1.6",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "custom-defaults": "^0.1.2",
    "rollup": "^2.45.2",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "peerDependencies": {
    "rollup": "^2.45.2"
  }
}
